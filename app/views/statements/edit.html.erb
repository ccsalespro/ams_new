<p><%= @var %></p>
<div class="container">
<%= render 'long_form' %>
</div>
<table class="table table-striped table-bordered">
   <tr>
   	   <th>IntType</th>
	   <th>IntType</th>
	   <th>Percent</th>
	   <th>Per Item</th>
	   <th>Transactions</th>
	   <th>Volume</th>
	   <th>Fees</th>
   </tr>

<% @inttableitems = Inttableitem.where(statement_id: @statement.id) %>
<% @inttableitems.each do |item| %>
	<% @inttype = Inttype.find_by_id(item.inttype_id) %>
	<tr>
		<td><%= @inttype.id %></td>
		<td><%= @inttype.description %></td>
		<td><%= @inttype.percent %></td>
		<td><%= @inttype.per_item %></td>
		<td><%= item.transactions %></td>
		<td><%= item.volume %></td>
		<td><%= item.costs %></td>
	</tr>
<% end %>
</table>
