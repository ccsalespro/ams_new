<p id="notice"><%= notice %></p>

<h1>Listing Inttableitems</h1>

<table>
  <thead>
    <tr>
      <th>Inttype</th>
      <th>Statement</th>
      <th>Transactions</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inttableitems.each do |inttableitem| %>
      <tr>
        <td><%= inttableitem.inttype %></td>
        <td><%= inttableitem.statement %></td>
        <td><%= inttableitem.transactions %></td>
        <td><%= inttableitem.volume %></td>
        <td><%= link_to 'Show', inttableitem %></td>
        <td><%= link_to 'Edit', edit_inttableitem_path(inttableitem) %></td>
        <td><%= link_to 'Destroy', inttableitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inttableitem', new_inttableitem_path %>
