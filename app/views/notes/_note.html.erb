<%= div_for(note) do %>
	<div class="clearfix delete note">
	<hr class="style-two">
		<div class="">
		<div class="btn-group pull-right">
				<%= link_to note, method: :delete, :class => 'delete-note', :remote => true do %>
				    <i class="fa fa-trash big-fa black" aria-hidden="true"></i>
				<% end %>
		</div>
		<p class="text-left"><small class="text-capitalize"><strong><%= time_ago_in_words(note.created_at) %> Ago</strong></small></p>
			<p class="text-left"><%= note.body %></p>
			
		</div>
	</div>
<% end %>