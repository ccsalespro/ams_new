<p id="notice"><%= notice %></p>

<h1>Listing Intcalcitems</h1>

<table>
  <thead>
    <tr>
      <th>Inttype</th>
      <th>Transactions</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intcalcitems.each do |intcalcitem| %>
      <tr>
        <td><%= intcalcitem.inttype %></td>
        <td><%= intcalcitem.transactions %></td>
        <td><%= intcalcitem.volume %></td>
        <td><%= link_to 'Show', intcalcitem %></td>
        <td><%= link_to 'Edit', edit_intcalcitem_path(intcalcitem) %></td>
        <td><%= link_to 'Destroy', intcalcitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intcalcitem', new_intcalcitem_path %>
