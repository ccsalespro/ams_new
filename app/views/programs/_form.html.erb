<%= form_for([@processor, @program]) do |f| %>
  <% if @program.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@program.errors.count, "error") %> prohibited this program from being saved:</h2>

      <ul>
      <% @program.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div class="container">
  <div class="col-md-6">
    <div class="form-group">
      <%= f.label :name %><br>
      <%= f.text_field :name, class: "form-control", required: true %>
    </div>
    <div class="form-group">
      <%= f.label :min_volume %><br>
      <%= f.number_field :min_volume, class: "form-control", value: 3000, :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :max_volume %><br>
      <%= f.number_field :max_volume, class: "form-control", value: 30000, :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :up_front_bonus %><br>
      <%= f.number_field :up_front_bonus, class: "form-control", value: 200, :required => true %>
    </div>
    <div class="form-group">
      <p class="bold ">Residual Split %</p>
      <%= f.number_field :residual_split, class: "form-control", value: 50, :min => 5, :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_bp_mark_up %><br>
      <%= f.number_field :min_bp_mark_up, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_per_item_fee %><br>
      <%= f.number_field :min_per_item_fee, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :cost_structure %><br>
      <%= collection_select( :program, :structure_id, Structure.all, :id, :name, {}, { :multiple => false }) %>
    </div>
    <div class="form-group">
      <%= f.label :terminal_type %><br>
      <%= collection_select( :program, :system_id, System.all, :id, :name, {}, { :multiple => false }) %>
    </div>
    <div class="form-group">
      <%= f.label :terminal_ownership_type %><br>
      <%= f.text_field :terminal_ownership_type, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_per_batch_fee %><br>
      <%= f.number_field :min_per_batch_fee, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :per_batch_cost %><br>
      <%= f.number_field :per_batch_cost, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :per_item_cost %><br>
      <%= f.number_field :per_item_cost, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <p class="bold ">Bin Sponsorship Basis Points</p>
      <%= f.number_field :bin_sponsorship, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :visa_access_per_item %><br>
      <%= f.number_field :visa_access_per_item, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <p class="bold ">Visa Access Basis Points</p>
      <%= f.number_field :visa_access_percentage, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :mc_access_per_item %><br>
      <%= f.number_field :mc_access_per_item, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <p class="bold ">Mastercard Access Basis Points</p>
      <%= f.number_field :mc_access_percentage, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :disc_access_per_item %><br>
      <%= f.number_field :disc_access_per_item, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
       <p class="bold ">Discover Access Basis Points</p>
      <%= f.number_field :disc_access_percentage, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_monthly_fees %><br>
      <%= f.number_field :min_monthly_fees, class: "form-control", :required => true, value: 9.95 %>
    </div>
    <div class="form-group">
      <%= f.label :monthly_fee_costs %><br>
      <%= f.number_field :monthly_fee_costs, class: "form-control", :required => true, value: 450 %>
    </div>
    <div class="form-group">
      <%= f.label :monthly_pci_fee %><br>
      <%= f.number_field :monthly_pci_fee, class: "form-control", :required => true, value: 9.95 %>
    </div>
    <div class="form-group">
      <%= f.label :monthly_pci_cost %><br>
      <%= f.number_field :monthly_pci_cost, class: "form-control", :required => true, value: 450 %>
    </div>
    <div class="form-group">
      <%= f.label :annual_pci_fee %><br>
      <%= f.number_field :annual_pci_fee, class: "form-control", :required => true, value: 0 %>
    </div>
    <div class="form-group">
      <%= f.label :annual_pci_cost %><br>
      <%= f.number_field :annual_pci_cost, class: "form-control", :required => true, value: 0 %>
    </div>
    <div class="form-group">
      <%= f.label :min_pin_debit_per_item_fee %><br>
      <%= f.number_field :min_pin_debit_per_item_fee, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :pin_debit_per_item_cost %><br>
      <%= f.number_field :pin_debit_per_item_cost, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_monthly_debit_fee %><br>
      <%= f.number_field :min_monthly_debit_fee, class: "form-control", :required => true, value: 9.95 %>
    </div>
    <div class="form-group">
      <%= f.label :monthly_debit_fee_cost %><br>
      <%= f.number_field :monthly_debit_fee_cost, class: "form-control", :required => true, value: 450 %>
    </div>
    <div class="form-group">
      <%= f.label :next_day_funding_monthly_fee %><br>
      <%= f.number_field :next_day_funding_monthly_fee, class: "form-control", :required => true, value: 9.95 %>
    </div>
    <div class="form-group">
      <%= f.label :next_day_funding_monthly_cost %><br>
      <%= f.number_field :next_day_funding_monthly_cost, class: "form-control", :required => true, value: 450 %>
    </div>
    <div class="form-group">
      <%= f.label :min_amex_per_item_fee %><br>
      <%= f.number_field :min_amex_per_item_fee, class: "form-control", :required => true, value: 0.15 %>
    </div>
    <div class="form-group">
      <%= f.label :amex_per_item_cost %><br>
      <%= f.number_field :amex_per_item_cost, class: "form-control", :required => true, value: 0.10 %>
    </div>
    <div class="form-group">
      <%= f.label :amex_bp_cost %><br>
      <%= f.number_field :amex_bp_cost, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :min_amex_bp_fee %><br>
      <%= f.number_field :min_amex_bp_fee, class: "form-control", :required => true %>
    </div>
    <div class="form-group">
      <%= f.label :application_fee_cost %><br>
      <%= f.number_field :application_fee_cost, class: "form-control", :required => true, value: 0 %>
    </div>
    <div class="form-group">
      <%= f.label :min_application_fee %><br>
      <%= f.number_field :min_application_fee, class: "form-control", :required => true, value: 0 %>
    </div>
    <div class="actions">
      <%= f.submit "Submit", class: "btn btn-lg btn-primary", :required => true %>
    </div>
    <% end %>
  </div>
</div>

